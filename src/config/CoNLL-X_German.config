[core]
learner = average_perceptron
parser = ceisner
feature_generator = english_1st_fgen

[data]
format = conllx
train  = german_tiger_train.conll
test   = german_tiger_test.conll|german_tiger_test_blind.conll
data_path = %(NATLANG_DATA)s/glm-parser-data/CoNLL-X/German/data/
prep_path = data/prep/
tag_file = tagset.txt

[option]
spark_shards = 4
iterations = 5
load_weight_from =
dump_weight_to =
dump_frequency = 1
tagger_w_vector =
